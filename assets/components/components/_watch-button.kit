<button class="o-button__icon-toggle o-button__mobile-toggle" v-on:click="toggleWatchStatus(lot)" v-bind:class="{ 's-active': watchedLots.indexOf(lot.lotNumber) >= 0}">
    <i class="icon-watch"></i>
    <span v-if="watchedLots.indexOf(lot.lotNumber) >= 0">Watching</span>
    <span v-else>Watch this lot</span>
</button>