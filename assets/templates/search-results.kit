<!DOCTYPE html>
<html>

<head>
	<link rel="manifest" href="./manifest.json">
    <title>RB TIMED AUCTION</title>
    <link type="text/css" rel="stylesheet" href="assets/css/style.css">

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8"> 
</head>

<body class="body USD">
	<div class="js--tal">
        <!-- @include '/assets/components/layout/header' -->
        
        <div class="l-main-container"> 
            <div class="js--nav-pin-waypoint"></div>
            <div class="l-pin-header-wrapper js--pinned-header">
                <div class="l-pin-header-inner">
                    <div class="l-scroll-revealable-header" v-bind:class="{'s-showWholeHeader' : showSecondaryHeader}">
                        <!-- @include '/assets/components/components/navigation_tabs' -->
                        <!-- @include '/assets/components/layout/search' -->
                    </div>
                    <!-- @include '/assets/components/layout/filter' -->
                    <div class="c-navigation__tertiary g-wrapper">
                        <button class="o-button__transparent o-button__narrow" v-on:click="gotoPage('index')"><i class="icon-Back-Arrow"></i><span class="h-hide-mobile"> Back</span></button>
                        <div v-if="searchResults.length > 0" class="g-cell t-h2 t-lh-300 h-color-white h-ta-center"><span class="h-hide-mobile"><span class="t-bold">{{ searchResults.length }} Results</span> found for: </span><span class="t-bold">"Caterpillar"</span></div>
                        <div v-else class="g-cell t-h2 t-bold t-lh-300 h-color-white h-ml-200">0 Results found</div>
                    </div>
                    <!-- @include '/assets/components/components/choice_create-controls' -->
                </div>
            </div>            
            
            <div v-if="searchResults.length > 0">
                <div v-for="(lot,index) in searchedLots" class="c-lot-listing" v-bind:class="lotStatus(lot)"> 
                    <!-- @include '/assets/components/components/lot_item' -->
                </div>
            </div>
            <div v-else class="l-container__medium">
                <div class="h-ta-center h-mt-500 h-mb-500">
                    <img src="assets/img/empty.png">
                    <h3 class="t-h3">We couldn't find any lots matching your request!</h3>
                    <button class="o-button__primary">Back to all listings</button>
                </div>
            </div>

        </div>

        <!-- @include '/assets/components/layout/footer_paginated' -->
        <footer>Prototype controls: <button v-on:click="emptySearch">Show Empty Version</button></footer>
        
	</div>
    <!-- @include '/assets/components/components/js-includes' -->

</body>
</html>